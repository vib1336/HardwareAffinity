@{
    this.ViewData["Title"] = "404 Error";
}

<head>
    <link href="~/css/notfound.css" rel="stylesheet"/>
</head>

<h1>404 Error Page</h1>
<p class="zoom-area"><b>Product</b> you were searching for was not found. </p>
<section class="error-container">
